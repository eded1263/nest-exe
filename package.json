{
  "name": "nest-exe",
  "version": "0.1.0",
  "description": "Demo to create EXE file with Node and Typescript",
  "scripts": {
    "clean": "rm -rf build dist",
    "compile": "nest build",
    "bundle": "webpack",
    "minify": "babel build/bundle.js --out-file dist/${npm_package_name}.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "dev": "nest start --watch",
    "start": "nest start",
    "build-all": "yarn lint && yarn test && yarn clean && yarn compile && yarn bundle && yarn minify",
    "build-exe": "yarn build-all && pkg -t latest-win-x64 --out-path dist/ dist/${npm_package_name}.js"
  },
  "author": "Gabriel Possamai & Edilson Santana",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "@babel/runtime-corejs3": "^7.22.15",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "core-js": "^3.32.2",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.14.0",
    "rxjs": "^7.8.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.16.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^27.3.1",
    "babel-preset-minify": "^0.5.1",
    "debug": "^4.3.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1"
  },
  "externalDependencies": [
    "@nestjs/microservices",
    "@nestjs/platform-express",
    "class-transformer",
    "class-validator",
    "cache-manager"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
